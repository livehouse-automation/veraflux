<?xml version="1.0" encoding="utf-8"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>5</minor>
  </specVersion>
  <actionList>
    <action>
      <name>SetStaticTags</name>
      <argumentList>
        <argument>
          <name>newStaticTagsValue</name>
          <direction>in</direction>
          <relatedStateVariable>StaticTags</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetStaticTags</name>
      <argumentList>
        <argument>
          <name>StaticTagsValue</name>
          <direction>out</direction>
          <relatedStateVariable>StaticTags</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetPeriod</name>
      <argumentList>
        <argument>
          <name>newPeriodValue</name>
          <direction>in</direction>
          <relatedStateVariable>Period</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetPeriod</name>
      <argumentList>
        <argument>
          <name>PeriodValue</name>
          <direction>out</direction>
          <relatedStateVariable>Period</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetFluxCapacitor</name>
      <argumentList>
        <argument>
          <name>newFluxCapacitorValue</name>
          <direction>in</direction>
          <relatedStateVariable>FluxCapacitor</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetFluxCapacitor</name>
      <argumentList>
        <argument>
          <name>FluxCapacitorValue</name>
          <direction>out</direction>
          <relatedStateVariable>FluxCapacitor</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetDebug</name>
      <argumentList>
        <argument>
          <name>newDebugValue</name>
          <direction>in</direction>
          <relatedStateVariable>Debug</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetDebug</name>
      <argumentList>
        <argument>
          <name>DebugValue</name>
          <direction>out</direction>
          <relatedStateVariable>Debug</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetInfluxDB</name>
      <argumentList>
        <argument>
          <name>newInfluxDBValue</name>
          <direction>in</direction>
          <relatedStateVariable>InfluxDB</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetInfluxDB</name>
      <argumentList>
        <argument>
          <name>InfluxDBValue</name>
          <direction>out</direction>
          <relatedStateVariable>InfluxDB</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetInfluxIP</name>
      <argumentList>
        <argument>
          <name>newInfluxIPValue</name>
          <direction>in</direction>
          <relatedStateVariable>InfluxIP</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetInfluxIP</name>
      <argumentList>
        <argument>
          <name>InfluxIPValue</name>
          <direction>out</direction>
          <relatedStateVariable>InfluxIP</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetInfluxPort</name>
      <argumentList>
        <argument>
          <name>newInfluxPortValue</name>
          <direction>in</direction>
          <relatedStateVariable>InfluxPort</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetInfluxPort</name>
      <argumentList>
        <argument>
          <name>InfluxPortValue</name>
          <direction>out</direction>
          <relatedStateVariable>InfluxPort</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetInfluxUser</name>
      <argumentList>
        <argument>
          <name>newInfluxUserValue</name>
          <direction>in</direction>
          <relatedStateVariable>InfluxUser</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetInfluxUser</name>
      <argumentList>
        <argument>
          <name>InfluxUserValue</name>
          <direction>out</direction>
          <relatedStateVariable>InfluxUser</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetInfluxPassword</name>
      <argumentList>
        <argument>
          <name>newInfluxPasswordValue</name>
          <direction>in</direction>
          <relatedStateVariable>InfluxPassword</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetInfluxPassword</name>
      <argumentList>
        <argument>
          <name>InfluxPasswordValue</name>
          <direction>out</direction>
          <relatedStateVariable>InfluxPassword</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
  </actionList>
<!-- State Variables -->
 <serviceStateTable>
    <stateVariable>
       <sendEventsAttribute>no</sendEventsAttribute>
       <name>Period</name>
       <dataType>i4</dataType>
       <defaultValue>60</defaultValue>
    </stateVariable>
    <stateVariable>
       <sendEventsAttribute>no</sendEventsAttribute>
       <name>StaticTags</name>
       <dataType>string</dataType>
       <defaultValue></defaultValue>
    </stateVariable>
    <stateVariable>
       <sendEventsAttribute>no</sendEventsAttribute>
       <name>InfluxDB</name>
       <dataType>string</dataType>
       <defaultValue>vera</defaultValue>
    </stateVariable>
    <stateVariable>
       <sendEventsAttribute>no</sendEventsAttribute>
       <name>FluxCapacitor</name>
       <dataType>boolean</dataType>
       <defaultValue>true</defaultValue>
    </stateVariable>
    <stateVariable>
       <sendEventsAttribute>no</sendEventsAttribute>
       <name>Debug</name>
       <dataType>boolean</dataType>
       <defaultValue>false</defaultValue>
    </stateVariable>
    <stateVariable>
       <sendEventsAttribute>no</sendEventsAttribute>
       <name>InfluxIP</name>
       <dataType>string</dataType>
       <defaultValue>Enter IP for InfluxDB</defaultValue>
    </stateVariable>
    <stateVariable>
       <sendEventsAttribute>no</sendEventsAttribute>
       <name>InfluxPort</name>
       <dataType>string</dataType>
       <defaultValue>Enter Port for InfluxDB</defaultValue>
    </stateVariable>
    <stateVariable>
       <sendEventsAttribute>no</sendEventsAttribute>
       <name>InfluxUser</name>
       <dataType>string</dataType>
       <defaultValue>Enter InfluxDB User Name</defaultValue>
    </stateVariable>
    <stateVariable>
       <sendEventsAttribute>no</sendEventsAttribute>
       <name>InfluxPassword</name>
       <dataType>string</dataType>
       <defaultValue>InfluxDB User Password</defaultValue>
    </stateVariable>
 </serviceStateTable>
</scpd>

